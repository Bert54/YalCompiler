programme test
debut

    fonction deux()
    debut
        retourne 2;
    fin

    fonction foisdeux(entier n)
    debut
        retourne n * 2;
    fin

    fonction melimelo(entier a ; entier b ; entier c ; entier d ; entier e)
    debut
        entier temp;
        temp = 10;
        si (a + -b > 0) alors
            temp = temp + foisdeux(d);
        finsi
        si (temp / deux() > d - (e + deux())) alors
        sinon
            a = -(a - b - c);
        finsi
        c = c / foisdeux(foisdeux(foisdeux(deux())));
        retourne a + b + c + d + e;
    fin

    fonction tripleaddition(entier u ; entier d ; entier t)
    debut
        retourne u + d + t;
    fin

    entier bite;
    entier penis;
    penis = -20;
    bite = tripleaddition(5, melimelo(4, bite, -1, 10, 2 + foisdeux(bite) / (-1)), penis);
